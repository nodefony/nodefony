{% include '../blocks/title.html.twig' with { 'title': 'SYSLOG ', 'subtitle': 'SYSLOG Nodefony framework ' } %}
<div class="wrapper" >
<div class="table-responsive"  >
	<table class="table table-striped table-bordered" cellspacing="0" width="100%">
		<thead>
			<tr>
				<th>Date</th>
				<th>When</th>
				<th>SEVERITY</th>
				<th>MSGID</th>
				<th>MODULE</th>
				<th>PAYLAOD</th>
			</tr>
		</thead>
		<tbody>
		{% for pdu in ringStack %}
		<tr>
			<td>{{pdu.date}}</td>
			<td>{{pdu.timeago}}</td>
			<td>{{pdu.severityName}}</td>
			<td>{{pdu.msgid}}</td>
			<td>{{pdu.moduleName}}</td>
			<td>{{pdu.payload}}</td>
		</tr>
		{% endfor %}
		</tbody>
	</table>
</div>
</div>
