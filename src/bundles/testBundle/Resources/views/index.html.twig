{% extends '/app/Resources/views/base.html.twig' %}

{% block title %}Welcome {{kernel.name}}! {% endblock %}

{% block stylesheets %}

	{{ parent() }}

	<!-- WEBPACK BUNDLE -->
	<link rel='stylesheet' href='{{CDN("stylesheet")}}/testBundle/assets/css/test.css' />

{% endblock %}

{% block body %}
	<div class='container'>
		<div class='row'>
			<img class='displayed' src='{{CDN("image")}}/frameworkBundle/images/nodefony-logo-white.png'>
			<h1 class="success" >
				<a href="https://nodefony.net">
					<strong itemprop="name" class="text-primary" style="font-size:45px">
						NODEFONY UNIT TEST
					</strong>
				</a>
			</h1>
		</div>
	</div>
	<div class='container'>
		<div class='row displayed'>
			{{readme}}
		</div>
	</div>
{% endblock %}

{% block javascripts %}

	{{ parent() }}

	<!-- WEBPACK BUNDLE -->
	<script src='{{CDN("javascript")}}/testBundle/assets/js/test.js'></script>

{% endblock %}
