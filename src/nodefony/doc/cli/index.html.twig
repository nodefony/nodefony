const nodefony = require("nodefony");

const project = new nodefony.cli("project", {
  signals: true,
  onStart: (cli) => {
    cli.idle();
    cli.log("start");
  },
  onSignal: (sig, cli) => {
    // you can trap here SIGINT SIGTERM SIGHUP SIGQUIT
    console.log(sig);
  }
});
