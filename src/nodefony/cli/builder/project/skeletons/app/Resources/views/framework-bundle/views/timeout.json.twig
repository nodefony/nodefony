{% extends "./layout.json.twig" %}

{% block body %}
  "server":"nodefony",
  {% if error.code %}
  "code":{{error.code}},
  {% else %}
  "code":408,
  {% endif %}
  {% if error.pdu %}
  "pdu":{{ error.pdu }},
  {% else %}
  "pdu":null,
  {% endif %}
  "url":"{{error.url }}",
  {% if error.message %}
  "message":"{{error.message }}"
  {% else %}
  "message":"Response Timeout"
  {% endif %}
  {% if error.stack %}
  "stack":"{{ error.stack|json_encode() }}"
  {% else %}
  "stack":null
  {% endif %}
{% endblock %}
