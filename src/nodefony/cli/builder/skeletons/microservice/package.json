{
  "name": "cci",
  "version": "1.0.0",
  "description": "Service description",
  "main": "/src/index.js",
  "browser": "./dist/cci.js",
  "private": true,
  "bin": {
    "cli": "./bin/cli"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "nodefony": "nodefony",
    "pm2": "pm2",
    "start": "node ./bin/cli",
    "dev": "NODE_ENV=development pm2 start config/pm2.config.js --env development --no-daemon",
    "dev_debug": "NODE_ENV=development; DEBUG=* pm2 start config/pm2.config.js --env development --no-daemon",
    "prod": "NODE_ENV=production pm2 start config/pm2.config.js --env production",
    "stop": "pm2 stop cci",
    "restart": "pm2 restart cci",
    "delete": "pm2 delete cci",
    "log": "pm2 logs",
    "monit": "pm2 monit",
    "status": "pm2 status",
    "kill": "pm2 kill",
    "build": "WEBPACK_ENV=production webpack --env.production --config config/webpack.config.js; WEBPACK_ENV=development webpack --env.development --verbose --config config/webpack.config.js",
    "test": "NODE_ENV=production mocha  --require @babel/register --recursive ./tests"
  },
  "keywords": [
    "javascript",
    "nodefony"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
    "node-gyp": "^6.1.0",
    "node-pre-gyp": "^0.14.0",
    "open": "^7.0.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "markdown-it": "^10.0.0",
    "nodefony": "^6.0.0-beta.2",
    "pm2": "^4.2.3",
    "pm2-logrotate": "^2.7.0",
    "socket.io": "^2.3.0"
  }
}
