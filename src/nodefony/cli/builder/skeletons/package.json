{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{name}} description",
  "author": "{{authorFullName}} <{{authorMail}}>",
{% if packageName == "app" %}
  "main": "appKernel.js",
{% else %}
  "main": "{{name}}Bundle.js",
{% endif %}
  "scripts": {},
  "private": true,
  "keywords": [
    "nodefony",
    "javascript",
    "{{name}}"
  ],
  "repository": {},
  "dependencies": {
{% if addons.workbox %}
    "workbox-webpack-plugin": "^4.3.1",
{% endif %}
{% if addons.bootstrap %}
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "font-awesome": "^4.7.0",
    "popper.js": "^1.16.0",
{% endif %}
{% if addons.webpack %}
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.2.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "raw-loader": "^3.1.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.3",
    "webpack": "^4.41.2",
    "webpack-merge": "^4.2.2"
{% endif %}
  },
  "devDependencies": {
    "node-gyp": "^6.0.0",
    "node-pre-gyp": "^0.13.0"
  },
  "license": "",
  "readmeFilename": "README.md",
  "contributors": []
}
