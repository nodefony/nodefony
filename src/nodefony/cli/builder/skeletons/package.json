{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{name}} description",
  "author": "{{authorName}} <{{authorEmail}}>",
{% if packageName == "app" %}
  "main": "appKernel.js",
{% else %}
  "main": "{{name}}Bundle.js",
{% endif %}
  "scripts": {},
  "private": true,
  "keywords": [
    "nodefony",
    "javascript",
    "{{name}}"
  ],
  "repository": {},
  "dependencies": {
{% if addons.workbox %}
    "workbox-webpack-plugin": "^6.3.0",
{% endif %}
{% if addons.bootstrap %}
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.2",
    "jquery": "^3.6.0",
    "font-awesome": "^4.7.0",
{% endif %}
{% if addons.webpack %}
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.3.0",
    "expose-loader": "^3.0.0",
    "html-webpack-plugin": "^5.3.2",
    "imports-loader": "^3.0.0",
    "mini-css-extract-plugin": "^2.4.1",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.57.1",
    "webpack-merge": "^5.8.0"
{% endif %}
  },
  "devDependencies": {
{% if addons.webpack %}
    "webpack-dev-server": "^4.3.1",
{% endif %}
    "@mapbox/node-pre-gyp": "^1.0.5",
    "node-gyp": "^8.2.0"
  },
  "license": "",
  "readmeFilename": "README.md",
  "contributors": []
}
