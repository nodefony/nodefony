{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{name}} description",
  "author": "{{authorName}} <{{authorEmail}}>",
{% if packageName == "app" %}
  "main": "appKernel.js",
{% else %}
  "main": "{{name}}Bundle.js",
{% endif %}
  "scripts": {},
  "private": true,
  "keywords": [
    "nodefony",
    "javascript",
    "{{name}}"
  ],
  "repository": {},
  "dependencies": {
{% if addons.workbox %}
    "workbox-webpack-plugin": "6.5.4",
{% endif %}
{% if addons.bootstrap %}
    "@popperjs/core": "2.11.6",
    "bootstrap": "^5.2.2",
    "jquery": "3.6.1",
    "font-awesome": "^4.7.0",
{% endif %}
{% if addons.webpack %}
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^9.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "6.7.1",
    "expose-loader": "4.0.0",
    "favicons": "6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "4.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "sass": "1.55.0",
    "sass-loader": "^13.1.0",
    "sitemap-webpack-plugin": "^1.1.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "5.3.6",
    "webpack": "5.74.0",
    "webpack-merge": "^5.8.0"
{% endif %}
  },
  "devDependencies": {
{% if addons.webpack %}
    "webpack-dev-server": "4.11.1",
{% endif %}
    "@mapbox/node-pre-gyp": "1.0.10",
    "node-gyp": "^9.3.0"
  },
  "license": "",
  "readmeFilename": "README.md",
  "contributors": []
}
