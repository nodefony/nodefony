{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{name}} description",
  "author": "{{authorName}} <{{authorEmail}}>",
{% if packageName == "app" %}
  "main": "appKernel.js",
{% else %}
  "main": "{{name}}Bundle.js",
{% endif %}
  "scripts": {},
  "private": true,
  "keywords": [
    "nodefony",
    "javascript",
    "{{name}}"
  ],
  "repository": {},
  "dependencies": {
{% if addons.workbox %}
    "workbox-webpack-plugin": "^5.1.3",
{% endif %}
{% if addons.bootstrap %}
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "font-awesome": "^4.7.0",
{% endif %}
{% if addons.webpack %}
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.6.0",
    "expose-loader": "^1.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "imports-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "raw-loader": "^4.0.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.7",
    "webpack": "^4.43.0",
    "webpack-merge": "^5.0.9"
{% endif %}
  },
  "devDependencies": {
    "node-gyp": "^7.0.0",
    "node-pre-gyp": "^0.15.0"
  },
  "license": "",
  "readmeFilename": "README.md",
  "contributors": []
}
