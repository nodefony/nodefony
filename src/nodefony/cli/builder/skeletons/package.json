{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{name}} description",
  "author": "{{authorName}} <{{authorEmail}}>",
{% if packageName == "app" %}
  "main": "appKernel.js",
{% else %}
  "main": "{{name}}Bundle.js",
{% endif %}
  "scripts": {},
  "private": true,
  "keywords": [
    "nodefony",
    "javascript",
    "{{name}}"
  ],
  "repository": {},
  "dependencies": {
{% if addons.workbox %}
    "workbox-webpack-plugin": "^6.4.2",
{% endif %}
{% if addons.bootstrap %}
    "@popperjs/core": "^2.11.2",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "font-awesome": "^4.7.0",
{% endif %}
{% if addons.webpack %}
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "6.6.0",
    "expose-loader": "^3.1.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "^3.1.1",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.4.0",
    "sitemap-webpack-plugin": "^1.1.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "webpack": "5.68.0",
    "webpack-merge": "^5.8.0"
{% endif %}
  },
  "devDependencies": {
{% if addons.webpack %}
    "webpack-dev-server": "4.7.4",
{% endif %}
    "@mapbox/node-pre-gyp": "^1.0.8",
    "node-gyp": "^8.4.1"
  },
  "license": "",
  "readmeFilename": "README.md",
  "contributors": []
}
