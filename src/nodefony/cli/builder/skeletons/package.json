{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{name}} description",
  "author": "{{authorName}} <{{authorEmail}}>",
{% if packageName == "app" %}
  "main": "appKernel.js",
{% else %}
  "main": "{{name}}Bundle.js",
{% endif %}
  "scripts": {},
  "private": true,
  "keywords": [
    "nodefony",
    "javascript",
    "{{name}}"
  ],
  "repository": {},
  "dependencies": {
{% if addons.workbox %}
    "workbox-webpack-plugin": "^6.1.0",
{% endif %}
{% if addons.bootstrap %}
    "bootstrap": "^4.6.0",
    "jquery": "^3.5.1",
    "font-awesome": "^4.7.0",
{% endif %}
{% if addons.webpack %}
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^5.0.1",
    "expose-loader": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0",
    "imports-loader": "^2.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "popper.js": "^1.16.1",
    "postcss": "^8.2.4",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "raw-loader": "^4.0.2",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.20.1",
    "webpack-merge": "^5.7.3"
{% endif %}
  },
  "devDependencies": {
    "node-gyp": "^7.1.2",
    "node-pre-gyp": "^0.17.0"
  },
  "license": "",
  "readmeFilename": "README.md",
  "contributors": []
}
