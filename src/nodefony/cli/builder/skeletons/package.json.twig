{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{name}} description",
  "author": "{{authorName}} <{{authorEmail}}>",
{% if packageName == "app" %}
  "main": "appKernel.js",
{% else %}
  "main": "{{name}}Bundle.js",
{% endif %}
  "scripts": {},
  "private": true,
  "keywords": [
    "nodefony",
    "javascript",
    "{{name}}"
  ],
  "repository": {},
  "dependencies": {},
  "devDependencies": {
{% if addons.workbox %}
    "workbox-webpack-plugin": "6.5.4",
{% endif %}
{% if addons.bootstrap %}
    "@popperjs/core": "2.11.7",
    "bootstrap": "^5.2.3",
    "jquery": "3.6.4",
    "font-awesome": "^4.7.0",
{% endif %}
{% if addons.webpack %}
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "6.7.3",
    "expose-loader": "4.1.0",
    "favicons": "7.1.2",
    "favicons-webpack-plugin": "6.0.0",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "4.0.1",
    "mini-css-extract-plugin": "2.7.5",
    "sass": "1.62.0",
    "sass-loader": "13.2.2",
    "sitemap-webpack-plugin": "^1.1.1",
    "style-loader": "3.3.2",
    "terser-webpack-plugin": "5.3.7",
    "webpack": "5.79.0",
    "webpack-dev-server": "4.13.2",
    "webpack-merge": "^5.8.0",
{% endif %}
    "@mapbox/node-pre-gyp": "1.0.10",
    "node-gyp": "^9.3.1"
  },
  "license": "",
  "readmeFilename": "README.md"
}
