{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "author": "{{authorFullName}} <{{authorMail}}>",
  "main": "/src/index.js",
  "browser": "./dist/index.js",
  "private": true,
  "bin": {
    "cli": "./bin/cli"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "nodefony": "nodefony",
    "pm2": "pm2",
    "install": "env-cmd -e webpack_prod webpack --env production --config config/webpack.config.js",
    "start": "node ./bin/cli",
    "dev": "env-cmd -e development pm2 start config/pm2.config.js --only {{name}} --env development --no-daemon",
    "dev_debug": "env-cmd -e development_debug pm2 start config/pm2.config.js --only {{name}} --env development --no-daemon",
    "prod": "env-cmd -e webpack_prod webpack --env production --config config/webpack.config.js;env-cmd -e production pm2 start config/pm2.config.js --env production",
    "examples": "env-cmd -e development pm2 start config/pm2.config.js --only examples --env development --no-daemon",
    "stop": "pm2 stop {{name}}",
    "restart": "pm2 restart {{name}}",
    "delete": "pm2 delete {{name}}",
    "log": "pm2 logs",
    "monit": "pm2 monit",
    "status": "pm2 status",
    "kill": "pm2 kill",
    "webpack": "env-cmd -e webpack_prod webpack --env production --stats verbose --config config/webpack.config.js",
    "ssl": "./bin/generateCertificates.sh",
    "test": "env-cmd -e test mocha  --require @babel/register --recursive ./tests"
  },
  "keywords": [
    "javascript",
    "node.js",
    "nodefony",
    "microservice"
  ],
  "dependencies": {
    "ejs": "^3.1.6",
    "env-cmd": "^10.1.0",
    "hello_world": "./src/n-api",
    "markdown-it": "^12.2.0",
    "nodefony": "^{{version}}",
    "nodefony-client": "^6.0.0-beta.18",
    "pm2": "^5.1.2",
    "pm2-logrotate": "^2.7.0",
    "serve-static": "^1.14.1",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2 "
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@mapbox/node-pre-gyp": "^1.0.6",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.3",
    "mocha": "^9.1.3",
    "node-gyp": "^8.3.0",
    "open": "^8.4.0",
    "sass-loader": "^12.2.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "license": "ISC"
}
