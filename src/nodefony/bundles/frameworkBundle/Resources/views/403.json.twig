{% extends "./layout.json.twig" %}

{% block body %}
"server":"nodefony",
"code":403,
"url":"{{ url }}",
"bundle":"{{ bundle }}",
"controller":"{{ Controller }}",
{% if exception.message %}
"message":"{{  exception.message or "Forbidden"  }}"
{% else %}
"message":"{{ message or "Forbidden"  }}"
{% endif %}

{% endblock %}
