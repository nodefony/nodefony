{% extends "./layout.json.twig" %}

{% block body %}
"server":"nodefony",
"code":500,
"url":"{{ url }}",
"bundle":"{{ bundle }}",
"controller":"{{ Controller }}",
{% if exception.message %}
"message":"{{  exception.message or "Internal Server Error"  }}"
{% else  %}
"message":"{{ exception|json_encode() or "Internal Server Error"  }}"
{% endif %}

{% endblock %}
