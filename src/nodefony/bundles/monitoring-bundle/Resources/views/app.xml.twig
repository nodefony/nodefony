<?xml version="1.0" encoding="UTF-8"?>
<module id="app" version="1.0" src="/monitoring-bundle/modules/app/appModule.js" type="application/javascript">

  <content>
    <dashboard layout="standard"></dashboard>

    <components>
      <component label="Nodefony" icon="glyphicon glyphicon-envelope"></component>
    </components>

    <bundles>
      {% for bundle, value in bundles %}
        <bundle name="{{value.name}}">
          {{value.name}}
        </bundle>
      {% endfor %}
    </bundles>

    <user>
      {% for ele, value in user %}
        <{{ele}}>{{value}}</{{ele}}>
      {% endfor %}
    </user>

  </content>

  <controllers>
    <controller name="appController" src="/monitoring-bundle/modules/app/controller/appController.js" type="application/javascript"/>
    <controller name="navController" src="/monitoring-bundle/modules/app/controller/navController.js" type="application/javascript"/>
    <controller name="dashboardController" src="/monitoring-bundle/modules/app/controller/dashboardController.js" type="application/javascript"/>
    <controller name="testsController" src="/monitoring-bundle/modules/app/controller/testsController.js" type="application/javascript"/>
  </controllers>

  <templates>
    <template name="index" src="/monitoring-bundle/modules/app/Resources/views/index.html.twig" type="application/twig"/>
    <template name="404" src="/monitoring-bundle/modules/app/Resources/views/404.html.twig" type="application/twig"/>
    <template name="500" src="/monitoring-bundle/modules/app/Resources/views/500.html.twig" type="application/twig"/>
    <template name="navigation" src="/monitoring-bundle/modules/app/Resources/views/nav.html.twig" type="application/twig"/>
    <template name="dashboard" src="/monitoring-bundle/modules/app/Resources/views/dashboard.html.twig" type="application/twig"/>

    <template name="widget" src="/monitoring-bundle/modules/app/Resources/views/developer/widget.html.twig" type="application/twig"/>
    <template name="button" src="/monitoring-bundle/modules/app/Resources/views/developer/button.html.twig" type="application/twig"/>
    <template name="color" src="/monitoring-bundle/modules/app/Resources/views/developer/color.html.twig" type="application/twig"/>
    <template name="domparser" src="/monitoring-bundle/modules/app/Resources/views/developer/domparser.html.twig" type="application/twig"/>
    <template name="realTime" src="/monitoring-bundle/modules/app/Resources/views/developer/realTime.html.twig" type="application/twig"/>
    <template name="graph" src="/monitoring-bundle/modules/app/Resources/views/developer/graph.html.twig" type="application/twig"/>

    <template name="routing" src="/monitoring-bundle/modules/app/Resources/views/route/routing.html.twig" type="application/twig"/>
    <template name="services" src="/monitoring-bundle/modules/app/Resources/views/services/services.html.twig" type="application/twig"/>
    <template name="syslog" src="/monitoring-bundle/modules/app/Resources/views/syslog/index.html.twig" type="application/twig"/>
    <template name="kernel" src="/monitoring-bundle/modules/app/Resources/views/kernel/kernel.html.twig" type="application/twig"/>
    <template name="bundle" src="/monitoring-bundle/modules/app/Resources/views/bundles/bundle.html.twig" type="application/twig"/>
    <template name="requests" src="/monitoring-bundle/modules/app/Resources/views/request/requests.html.twig" type="application/twig"/>
    <template name="request" src="/monitoring-bundle/modules/app/Resources/views/request/request.html.twig" type="application/twig"/>
    <template name="response" src="/monitoring-bundle/modules/app/Resources/views/request/response.html.twig" type="application/twig"/>
    <template name="messagesWs" src="/monitoring-bundle/modules/app/Resources/views/request/messagesWs.html.twig" type="application/twig"/>
    <template name="session" src="/monitoring-bundle/modules/app/Resources/views/request/session.html.twig" type="application/twig"/>
    <template name="twig" src="/monitoring-bundle/modules/app/Resources/views/request/twig.html.twig" type="application/twig"/>
    <template name="security" src="/monitoring-bundle/modules/app/Resources/views/request/security.html.twig" type="application/twig"/>
    <template name="sequelize" src="/monitoring-bundle/modules/app/Resources/views/request/sequelize.html.twig" type="application/twig"/>
    <template name="performance" src="/monitoring-bundle/modules/app/Resources/views/request/performance.html.twig" type="application/twig"/>
    <template name="events" src="/monitoring-bundle/modules/app/Resources/views/request/events.html.twig" type="application/twig"/>
    <template name="activity" src="/monitoring-bundle/modules/app/Resources/views/activity.html.twig" type="application/twig"/>
    <template name="firewall" src="/monitoring-bundle/modules/app/Resources/views/security/security.html.twig" type="application/twig"/>
    <template name="tests" src="/monitoring-bundle/modules/app/Resources/views/test/index.html.twig" type="application/twig"/>

    <template name="users" src="/monitoring-bundle/modules/app/Resources/views/users.html.twig" type="application/twig"/>
    <template name="sessions" src="/monitoring-bundle/modules/app/Resources/views/sessions.html.twig" type="application/twig"/>
  </templates>

  <scripts></scripts>

  <translations>
    <translation src="/monitoring-bundle/modules/app/Resources/translations/app.fr_FR.json" type="json"/>
  </translations>

  <modules></modules>

  <styles></styles>

  <routes>
    <route id="index" path="/">
      <default key="controller">appModule:app:index</default>
    </route>
    <route id="dashboard" path="/dashboard">
      <default key="controller">appModule:dashboard:index</default>
    </route>
    <route id="developer" path="/developer/{element}">
      <default key="controller">appModule:app:developer</default>
    </route>
    <route id="parameters" path="/parameters/{module}">
      <default key="controller">appModule:app:parameters</default>
    </route>
    <route id="help" path="/help">
      <default key="controller">appModule:app:help</default>
    </route>

  </routes>
</module>