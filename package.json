{
  "name": "nodefony-core",
  "version": "7.0.0",
  "description": "Core Nodefony Node.js Web Framework",
  "main": "./src/nodefony/core/autoloader.es6",
  "scripts": {
    "nodefony": "node src/nodefony/bin/nodefony",
    "pm2": "node src/nodefony/node_modules/.bin/pm2",
    "start": "node src/nodefony/bin/nodefony dev",
    "dev": "node src/nodefony/bin/nodefony dev",
    "outdated": "npm outdated --deph=0",
    "build": "node src/nodefony/bin/nodefony build",
    "test": "node src/nodefony/bin/nodefony unitest:launch:all",
    "publish": "bash bin/publish.sh",
    "certificates": "bash bin/generateCertificates.sh",
    "changelog": "gren changelog --username=nodefony  --repo=nodefony --tags=all --data-source=commits --override --generate"
  },
  "private": true,
  "bin": {
    "nodefony": "src/nodefony/bin/nodefony"
  },
  "keywords": [
    "nodefony",
    "node.js",
    "node",
    "framework",
    "webpack",
    "websocket",
    "realtime"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/nodefony/nodefony.git"
  },
  "bugs": {
    "url": "https://github.com/nodefony/help/issues"
  },
  "licenses": [
    {
      "type": "CECILL-B",
      "url": "http://www.cecill.info/licences/Licence_CeCILL-B_V1-en.html"
    }
  ],
  "os": [
    "darwin",
    "linux",
    "freebsd",
    "win32"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "graphql": "16.8.1",
    "mongoose": "^6.6.5",
    "pm2": "5.2.0",
    "pm2-logrotate": "^2.7.0",
    "sequelize": "^6.24.0",
    "webpack": "5.74.0",
    "webpack-dev-server": "4.11.1"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "1.0.10",
    "editorconfig": "^0.15.3",
    "github-release-notes": "^0.17.3",
    "jshint": "2.13.5",
    "node-gyp": "^9.2.0"
  },
  "author": "Christophe CAMENSULI <ccamensuli@gmail.com>",
  "readmeFilename": "README.md",
  "contributors": [
    {}
  ]
}
